<table>
  <tr>
    <th>Name</th>
    <th>Personnel No</th>
    <th>Job Description</th>

  </tr>


  <% @personnels.each do |p| %>
    <tr class= <%= cycle("odd","even")%>>
      <td>
        <%= p.name %>
      </td>
      <td>
        <%= p.pno%>
      </td>
      <td>
        <%=p.jobdescription %>
      </td>

    </tr>
  <% end %>
</table>
